using System;
using System.Collections.Generic;
using System.Linq;
using System.Text.RegularExpressions;

namespace HelloWorld
{
	public class Program
	{
	  public int GreatestNumber(int num1,int num2){
	    if(num1 == num2){
	      return num1;
	    }
	    else if(num1 > num2){
	      return num1;
	    }
	    return num2;
	  }
		public static void Main(string[] args)
		{
		  Program p = new Program();
		  int num1,num2;
		  num1 =  Convert.ToInt32(Console.ReadLine());
		  num2 =  Convert.ToInt32(Console.ReadLine());
			Console.WriteLine("Greatest Number is {0}",p.GreatestNumber(num1,num2));
		}
	}
}

C++ Code
int GreatestNumber(int num1,int num2){
	    if(num1 == num2){
	      return num1;
	    }
	    else if(num1 > num2){
	      return num1;
	    }
	    return num2;
	  }
int main() {
    int num1,num2;
    cin>>num1>>num2;
    cout<<"Greatest Number is "<<GreatestNumber(num1,num2);
}

Javascript Code

function GreatestNumber(num1,num2){
  if(num1===num2 || num1>num2){
      return num1;
  }
  return num2;
}
console.log(`Greatest Number is ${GreatestNumber(3,2)}`)
