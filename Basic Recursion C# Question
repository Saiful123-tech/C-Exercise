Print 1 To N Without Loop

public void printNos(int N)
    {
        //Your code here
        if(N==0){
            return;
        }
        printNos(N-1);
        Console.Write(N);
        Console.Write(" ");
    }

Print GFG n times

public void printGfg(int N)
    {
        //Your code here
        if(N==0){
            return;
        }
        Console.Write("GFG ");
        printGfg(N-1);
    }

TC o(n)
SC o(n) recursive

 public void printNos(int N)
    {
        //Your code here
        if(N==0){
            return;
        }
        Console.Write(N);
        Console.Write(" ");
        printNos(N-1);
    }

TC o(n)
SC o(n) recursive

